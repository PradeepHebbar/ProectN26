<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="BestBuyAPIPlayground" parallel="classes" thread-count="1"
	configfailurepolicy="continue">
	<test name="ProductAPIs" preserve-order="false">
		<groups>
			<run>
				<exclude name="sequential" />
				<exclude name="broken" />
				<exclude name="ignore" />
			</run>
		</groups>
		<packages>
			<package name="com.n26.tests.*" />
		</packages>
	</test>
</suite>